TYPE = &TYPE,
TYPE_NAME = &TYPE_NAME,
DATA_TYPE = &DATA_TYPE,
-:RPC_CATALOGER_RPCA3A_LABDONE1
LENGTH = &LENGTH,
PRECISION = &PRECISION,
NULLABLE = &NULLABLE,
SCALE = &SCALE,
RADIX = &RADIX,
PARM_ORDER = &PARM_ORDER,
PARM_DESC = &PARM_DESC,$
