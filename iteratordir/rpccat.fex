RPC_NAME=&RPC_NAME,
CREATOR=&CREATOR,
LOCATION=&LOCATION,
NUM_IPARMS=&NUM_IPARMS,
NUM_OPARMS=&NUM_OPARMS,
NUM_RESULTS=&NUM_RESULTS,
PID=&PID,
RPC_DESC=&RPC_DESC,
RPC_ALIAS=&RPC_ALIAS,$
END
-RUN
-IF &NEXT_ACTION NE 'delete' THEN GOTO :RPC_CATALOGER_LABSEG02 ;
EX &RPCCATALOGER
-EXIT
