FILENAME=TRADES, SUFFIX=FOC, REMARKS='Legacy Metadata Sample: trades',$
SEGNAME=TRADES, SEGTYPE=S0
 FIELD=TRADER_ID, ALIAS=TRADER_ID, FORMAT=A5, TITLE='Broker ID',
   DESC='Broker ID',$
 FIELD=DATE_OF_TRADE, ALIAS=DATE_OF_TRADE, FORMAT=MDYY, TITLE='Date of ,Trade',
   DESC='Date of Trade',$
 FIELD=CONTINENT, ALIAS=CONTINENT, FORMAT=A10, TITLE='Continent',
   DESC='Continent',$
