   DESC='Type of Instrument',$
 FIELD=BUY_SELL, ALIAS=BUY_SELL, FORMAT=A1, TITLE='Buy /,Sell',
   DESC='Buy or Sell',$
 FIELD=AMOUNT, ALIAS=AMOUNT, FORMAT=D16, TITLE='Amount', DESC='Amount',$
 DEFINE QTR/QYY = DATE_OF_TRADE ;$
 DEFINE MONTH/MYY = DATE_OF_TRADE ;$
 DEFINE BUY_AMOUNT = IF BUY_SELL EQ 'B' THEN AMOUNT ELSE 0;
 DEFINE SELL_AMOUNT = IF BUY_SELL EQ 'S' THEN AMOUNT ELSE 0;
 DEFINE ARGENTINA_PESO/D16 =    AMOUNT * 1.00;$
 DEFINE BRAZIL_REAL/D16 =       AMOUNT * 1.13;$
 DEFINE CANADA_DOLLAR/D16 =     AMOUNT * 1.40;$
