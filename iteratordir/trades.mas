 DEFINE GUATEMALA_QUETZAL/D16 = AMOUNT * 8.23;$
 DEFINE HONDURASLEMPIRAS/D16 =  AMOUNT * 8.50;$
 DEFINE HONG_KONG_DOLLAR/D16 =  AMOUNT * 7.74;$
 DEFINE ISRAEL_SHECKEL/D16 =    AMOUNT * 3.57;$
 DEFINE JAPAN_YEN/D16 =         AMOUNT * 126.27;$
 DEFINE MEXICO_PESO/D16 =       AMOUNT * 8.58;$
